{
  "name": "l2js-client",
  "version": "1.0.5",
  "chronicle": "High Five",
  "protocols": [
    268,
    271,
    273
  ],
  "description": "Lineage 2 Client Library",
  "author": "Nikolay Petrovski <to.petrovski@gmail.com>",
  "funding": "https://www.paypal.com/donate?hosted_button_id=9HF7J3CLAXDBG",
  "license": "MIT",
  "main": "dist/Client.js",
  "types": "dist/Client.d.ts",
  "scripts": {
    "compile": "node build-indexes.js && tsc",
    "clean": "rm -rf dist",
    "prebuildpackage": "npm run clean && npm run compile && node dist/SetupPackage.js",
    "buildpackage": "cd dist && npm pack",
    "lint": "eslint ./src/**",
    "dev:docs": "vuepress dev vuePress",
    "build:docs": "vuepress build vuePress",
    "publish": "npm publish --access public",
    "client:dev": "concurrently -k \"npm:client:dev:renderer\" \"npm:client:dev:electron\"",
    "client:dev:renderer": "vite --config renderer/vite.config.ts",
    "client:dev:electron": "wait-on tcp:5173 && cross-env TS_NODE_PROJECT=electron/tsconfig.json ELECTRON_RENDERER_URL=http://localhost:5173 electron -r ts-node/register ./electron/main.ts"
  },
  "keywords": [
    "lineage2",
    "nodejs",
    "typescript",
    "packets",
    "bot",
    "l2js"
  ],
  "prettier": {
    "trailingComma": "es5",
    "singleQuote": false,
    "useTabs": false,
    "tabWidth": 2,
    "printWidth": 120,
    "semi": true,
    "bracketSpacing": true,
    "bracketSameLine": false,
    "arrowParens": "always",
    "endOfLine": "lf"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/npetrovski/l2js-client"
  },
  "bugs": {
    "url": "https://github.com/npetrovski/l2js-client/issues"
  },
  "homepage": "https://github.com/npetrovski/l2js-client",
  "devDependencies": {
    "@babylonjs/core": "^7.9.0",
    "@types/linkify-it": "^5.0.0",
    "@types/node": "^20.11.28",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "electron": "^29.4.6",
    "eslint": "^8.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^4.7.3",
    "vite": "^5.0.12",
    "vuepress": "^1.8.2",
    "wait-on": "^7.2.0"
  },
  "dependencies": {
    "linkify-it": "^5.0.0"
  }
}
